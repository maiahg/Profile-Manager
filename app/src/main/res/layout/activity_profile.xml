<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">

    <ImageView
        android:id="@+id/teamid00"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="@+id/teamid03"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/teamid05"
        app:layout_constraintTop_toBottomOf="@+id/teamid03"
        app:srcCompat="@drawable/ic_logo_00"
        android:onClick="SetTeamIcon"/>

    <ImageView
        android:id="@+id/teamid01"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtSelectAvatar"
        app:srcCompat="@drawable/ic_logo_01"
        android:onClick="SetTeamIcon"/>

    <ImageView
        android:id="@+id/teamid03"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toEndOf="@+id/teamid02"
        app:layout_constraintTop_toTopOf="@+id/teamid02"
        app:srcCompat="@drawable/ic_logo_03"
        android:onClick="SetTeamIcon"/>

    <ImageView
        android:id="@+id/teamid05"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:onClick="SetTeamIcon"
        app:layout_constraintStart_toEndOf="@+id/teamid04"
        app:layout_constraintTop_toBottomOf="@+id/teamid02"
        app:srcCompat="@drawable/ic_logo_05" />

    <ImageView
        android:id="@+id/teamid02"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="40dp"
        android:onClick="SetTeamIcon"
        app:layout_constraintStart_toEndOf="@+id/teamid01"
        app:layout_constraintTop_toBottomOf="@+id/txtSelectAvatar"
        app:srcCompat="@drawable/ic_logo_02" />

    <ImageView
        android:id="@+id/teamid04"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="28dp"
        android:onClick="SetTeamIcon"
        app:layout_constraintStart_toStartOf="@+id/teamid01"
        app:layout_constraintTop_toBottomOf="@+id/teamid01"
        app:srcCompat="@drawable/ic_logo_04" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LOAD FROM PHONE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teamid05"
        app:layout_constraintVertical_bias="0.278" />

    <TextView
        android:id="@+id/txtSelectAvatar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="180dp"
        android:text="Select a new Avatar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>